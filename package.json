{
  "name": "wei-ui",
  "version": "0.0.1-alpha.0",
  "description": "基于elementUI-plus的高阶组件库",
  "private": false,
  "main": "dist/wei-ui.umd.js",
  "module": "dist/wei-ui.es.js",
  "style": "dist/style.css",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/wei-ui.es.js",
      "require": "./dist/wei-ui.umd.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "docs:dev": "vitepress dev docs",
    "build": "vue-tsc --noEmit && vite build ",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "build:production": "vue-tsc --noEmit && vite build --mode production"
  },
  "keywords": [
    "vue3",
    "vite",
    "component",
    "ui",
    "typescript"
  ],
  "author": "Wayne1308",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "typescript": "^4.8.4",
    "vitepress": "1.0.0-alpha.28"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vitepress-demo-preview/component": "^2.0.2",
    "@vitepress-demo-preview/plugin": "^1.1.5",
    "element-plus": "^2.2.21",
    "less": "^4.1.3",
    "vite": "^3.2.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Wayne1308/weiUI.git"
  }
}
